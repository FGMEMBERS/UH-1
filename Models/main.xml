<nasal>
<load>
var livery_update = aircraft.livery_update.new("Aircraft/UH-1/Models/Liveries", 10);

var self = cmdarg();

var float_0 = self.getNode("sim/multiplay/generic/float[0]", 1);
var float_1 = self.getNode("sim/multiplay/generic/float[1]", 1);
var float_2 = self.getNode("sim/multiplay/generic/float[6]", 1);
var float_3 = self.getNode("sim/multiplay/generic/float[7]", 1);
var float_4 = self.getNode("sim/multiplay/generic/float[8]", 1);
var float_5 = self.getNode("sim/multiplay/generic/float[9]", 1);

var leftFrontDoor_door = self.getNode("sim/model/door-positions/leftFrontDoor", 1);
var rightFrontDoor_door = self.getNode("sim/model/door-positions/rightFrontDoor", 1);
var leftSideDoor_door = self.getNode("sim/model/door-positions/leftSideDoor", 1);
var rightSideDoor_door = self.getNode("sim/model/door-positions/rightSideDoor", 1);
var leftSlideDoor_door = self.getNode("sim/model/door-positions/leftSlideDoor", 1);
var rightSlideDoor_door = self.getNode("sim/model/door-positions/rightSlideDoor", 1);

leftFrontDoor_door.alias(float_0);
rightFrontDoor_door.alias(float_1);
leftSideDoor_door.alias(float_2);
rightSideDoor_door.alias(float_3);
leftSlideDoor_door.alias(float_4);
rightSlideDoor_door.alias(float_5);
</load>

<unload>
livery_update.stop();
leftFrontDoor_door.unalias();
rightFrontDoor_door.unalias();
</unload>
</nasal>

