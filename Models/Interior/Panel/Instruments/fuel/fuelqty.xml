<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>fuelqty.ac</path>
	
  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>needle</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>glass.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <emission>
      <red>   0.028 </red>
      <green> 0.014 </green>
      <blue>  0.007 </blue>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>
	
  <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>/consumables/fuel/tank/level-lbs</property>
    <interpolation>
      <entry><ind>    0 </ind><dep>   0 </dep></entry>
      <entry><ind> 2000 </ind><dep> 240 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
    <center>
      <x-m>  0.000 </x-m>
      <y-m> -0.060 </y-m>
      <z-m> -0.003 </z-m>
    </center>
  </animation>

</PropertyList>

