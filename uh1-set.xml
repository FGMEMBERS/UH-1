<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList include="uh1-base.xml">

<sim>

<description>Bell UH 1 "Huey"</description>
<author>BARANGER Emmanuel (FMD/3D), StuartC (Twin version FDM), Heiko Schulz (3D/FDM), Simon Lister (3D), Maik Justus (FDM, sound)</author>


<!--MODEL-->
<flight-model>yasim</flight-model>
<aero>uh1-yasim</aero>

<!--STARTUP-->
<startup>
<splash-texture>Aircraft/UH-1/Splash/uh1-splash.png</splash-texture>
</startup>

<!--MODEL-->
<model>
<path archive="y">Aircraft/UH-1/Models/uh1.xml</path>
</model>

<!--SUBMODELS-->
<submodels>
<serviceable type="bool">true</serviceable>
<path>Aircraft/UH-1/Models/Effects/guns/UH-1submodels.xml</path>
</submodels>

<!--MULTIPLAY-->
<multiplay n="0">
<generic n="0">
<string n="1" type="string">uh1</string>
</generic>
</multiplay>

</sim>

<!--INSTRUMENTATION-->
<instrumentation>
<cdu>
<config>
<texture>boeing_brown.png</texture>
</config>
<display>IDENT</display>
<ident>
<model>uh1</model>
<engines>-80C2B1F</engines>
</ident>
<input type="string"/>
<serviceable>true</serviceable>
</cdu>
</instrumentation>

<!--NASAL_FILES-->
<nasal>
<radar>
<path>Aircraft/UH-1/Models/Interior/Panel/Instruments/RDR-160/RDR160.nas</path>
</radar>
</nasal>

</PropertyList>
