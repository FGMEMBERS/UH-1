<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList include="uh1-base.xml">

<sim>

<description>Bell 412 "Griffin"</description>
<author>BARANGER Emmanuel (FMD/3D), StuartC (Twin version, FDM), Heiko Schulz (3D/FDM), Simon Lister (3D), Maik Justus (FDM, sound)</author>
<status>V0.7</status>

<!--MODEL-->
<flight-model>yasim</flight-model>
<aero>412-yasim</aero>
<!--STARTUP-->
<startup>
<splash-texture>Aircraft/UH-1/Splash/412-splash.png</splash-texture>
</startup>

<!--MODEL-->
<model>
<path archive="y">Aircraft/UH-1/Models/412-Griffin.xml</path>
</model>

<!--SUBMODEL-->
<submodels>
<serviceable type="bool">true</serviceable>
<path>Aircraft/UH-1/Models/Effects/guns/UH-1submodels.xml</path>
</submodels>

<!-- Systems-->
	<systems>
      <electrical>
        <path>Aircraft/Generic/generic-electrical.xml</path>
      </electrical>
      <autopilot>
        <path>Aircraft/UH-1/System/autopilot-H.xml</path>
      </autopilot>
 
    
    </systems>

<!--MULTIPLAY-->
<multiplay n="0">
<generic n="0">
<string n="1" type="string">412</string>
</generic>
</multiplay>
</sim>

<!--INSTRUMENTATION-->
<instrumentation>
<cdu>
<config>
<texture>boeing_brown.png</texture>
</config>
<display>IDENT</display>
<ident>
<model>412</model>
<engines>-80C2B1F</engines>
</ident>
<input type="string"/>
<serviceable>true</serviceable>
</cdu>
</instrumentation>

<!--RADAR-->
<nasal>
<radar>
<path>Aircraft/UH-1/Models/Interior/Panel/Instruments/RDR-160/RDR160.nas</path>
</radar>
</nasal>

</PropertyList>
